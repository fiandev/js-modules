{
  "name": "@fiandev/js-modules",
  "version": "1.0.1",
  "description": "A gathering Javascript Modules And Algorithm",
  "main": "dist/main/index.cjs",
  "module": "dist/module/index.mjs",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest ./__test__/",
    "watch:test": "node --experimental-vm-modules node_modules/jest/bin/jest --watchAll",
    "build:module": "npx babel src --out-dir dist/module --env-name module --out-file-extension .mjs",
    "build:browser": "npx babel src --out-dir dist/main --env-name browser --out-file-extension .cjs",
    "build": "npm run build:module && npm run build:browser",
    "watch:build": "nodemon",
    "jsdoc": "jsdoc -r ./src --destination ./docs",
    "prettier": "prettier ./src/**/**/* -w",
    "prettier:test": "prettier ./__test__/**/*.js -w",
    "prettier:dist": "prettier ./dist/**/*.js -w",
    "prettier:docs": "prettier ./docs/**/*.{html,js,css} -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fiandev/js-modules.git"
  },
  "keywords": [
    "js",
    "library",
    "javascript",
    "tools"
  ],
  "engines": {
    "node": ">=6"
  },
  "author": "fiandev",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fiandev/js-modules/issues"
  },
  "homepage": "https://github.com/fiandev/js-modules#readme",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/preset-env": "^7.22.20",
    "@fiandev/js-modules": "^1.0.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-replace-import-extension": "^1.1.3",
    "babel-preset-minify": "^0.5.2",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2"
  },
  "files": [
    "dist/**/*.js",
    "!**/*.test.*",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  }
}
